(run){
  % general setting
  EVENTS 1M; ERROR 0.001; MI_HANDLER None; FRAGMENTATION Off;
  RANDOM_SEED 0;  SHOWER_GENERATOR None; SHERPA_LDADD SherpaMain;
  ME_SIGNAL_GENERATOR External Internal OpenLoops; INIT_ONLY 2;
  PDF_LIBRARY NNPDFSherpa;  PDF_SET NNPDF30NNLO;

  SCALES VAR{Abs2(p[0]+p[1])};

  % collider setup
  BEAM_1 2212 6500;
  BEAM_2 2212 6500; 

  OL_PARAMETERS check_collection 0;

}(run)

(processes){
  Process 1 -1 -> 11 -11 21;
  Loop_Generator Internal;
  Integrator Rambo;
  NLO_QCD_Mode Fixed_Order;
  NLO_QCD_Part RS;
  Min_Order (1,2);
  End process;
}(processes)

(selector){
#  NJetFinder 3 100.0 0.0 0.4 -1;
}(selector)
