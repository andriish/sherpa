(run){
  % general setting
  EVENTS 1M; ERROR 0.001; MI_HANDLER None; FRAGMENTATION Off;
  RANDOM_SEED 0;  SHOWER_GENERATOR None; SHERPA_LDADD SherpaMain;
  ME_SIGNAL_GENERATOR Amegic Internal; INIT_ONLY 2; AMEGIC_ALLOW_MAPPING 0;
  PDF_LIBRARY NNPDFSherpa;  PDF_SET NNPDF30NNLO;

  SCALES VAR{sqr(91.2)};

  % collider setup
  BEAM_1 2212 6500;
  BEAM_2 2212 6500; 

  DIPOLE_ALPHA 1.0; DIPOLE_AMIN 1.0e-99;

}(run)

(processes){
  Process 21 21 -> 21 21 21;
  Loop_Generator Internal;
  Integrator Rambo;
  NLO_QCD_Mode Fixed_Order;
  NLO_QCD_Part RS;
  Enable_MHV 0;
  Order (*,0);
  End process;
}(processes)

(selector){
#  NJetFinder 3 100.0 0.0 0.4 -1;
}(selector)
