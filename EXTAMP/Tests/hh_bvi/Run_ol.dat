(run){
  % general setting
  EVENTS 0; MI_HANDLER None; FRAGMENTATION Off; ERROR 1; FINISH_OPTIMIZATION Off;
  RANDOM_SEED 0;  SHOWER_GENERATOR None; INIT_ONLY 2; SHERPA_LDADD SherpaMain SherpaSM;
  ME_SIGNAL_GENERATOR External Internal OpenLoops; 
  PDF_LIBRARY LHAPDFSherpa; PDF_SET CT10nlo;
  
  SCALES VAR{sqr(91.2)};
  USE_DUMMY_VIRTUAL 1 1.0 1.0 1.0;

  % collider setup
  BEAM_1 2212; BEAM_ENERGY_1 = 6500.;
  BEAM_2 2212; BEAM_ENERGY_2 = 6500.;

}(run)

(processes){
  Process 1 -1 -> 1 -1 21 21;
  Loop_Generator Internal;
  Integrator Rambo;
  NLO_QCD_Mode Fixed_Order;
  NLO_QCD_Part BVI;
  Min_Order (4,0);
  End process;
}(processes)

(selector){
# NJetFinder 4 200.0 0.0 0.4 -1;
}(selector)
