(run){
  EVENTS 10000;
  NJET:=2; QCUT:=20;
  STABLE[6] 0; STABLE[24] 0;
  HARD_DECAYS 1;
  HARD_SPIN_CORRELATIONS 1;
  CORE_SCALE QCD;
}(run);

(beam){
  BEAM_1  2212; BEAM_ENERGY_1 980;
  BEAM_2 -2212; BEAM_ENERGY_2 980;
}(beam);

(processes){
