(run){
% general settings
  EVENTS 50k;

% model parameters
  ALPHAS(MZ) 0.1188;  
  ORDER_ALPHAS 2;
  CSS_FS_PT2MIN = 1.00
  ME_SIGNAL_GENERATOR Comix
% Has to stay until soft photons are fixed
  YFS_MODE = 0

% collider setup
  BEAM_1  11; BEAM_ENERGY_1 45.6;
  BEAM_2 -11; BEAM_ENERGY_2 45.6;
  PDF_LIBRARY PDFESherpa
  ISR_SMIN=sqr(20/E_CMS)
}(run)

(processes){
  Process 11 -11 -> 93 93
  Order (*,2);
  End process;
}(processes)

(selector){
  Mass 93 93 40. E_CMS
}(selector)